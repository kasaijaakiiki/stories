       $(document).ready(function() {
         $('#simple-menu').sidr({
          displace: false
         });
         $('.icon-close').click(function(){ // when 'icon-close' is clickedâ€¦
        $.sidr('close', 'sidr');  // close the div with the ID of "sidr"
          });
      });
     </script>
 </head>
<body>
  
<div id="sidr"> 
        <ul> 
          <li><a href="/">Home</a></li> 
          <li><a href="/about">About</a></li>
          <li><a href="/signups/new">Contact</a></li> 
        </ul> <%= image_tag('close.png', size: "30x40", :class => "icon-close", alt: "Close") %>

</div> 


<%= yield %>
  
  


